      "text": "Generate Email Response",
      "x": 240,
      "y": 320,
      "type": "execute-javascript",
      "icon": "javascript",
      "category": "utility",
      "parameters": {
        "code": "const priceData = {{processEthereumPrice.result}};\nconst emailBody = `Current Ethereum Price: ${priceData.price}\\n\\nTimestamp: ${priceData.timestamp}\\n\\nThis price information is provided by the CoinGecko API and is for informational purposes only.`;\nreturn { emailBody };"
      },
      "outputs": { "result": null, "error": "" },
      "description": "This utility node generates the email response with the formatted Ethereum price information.",
      "isSelected": false,
      "isActive": false,
      "error": null,
