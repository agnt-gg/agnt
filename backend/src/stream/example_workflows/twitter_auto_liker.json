      "icon": "javascript",
      "category": "utility",
      "parameters": {
        "code": "const tweetIds = {{extractTweetIds.result.tweetIds}};\nconst currentIndex = {{jsCounter.result.currentIndex}};\nconst currentTweetId = currentIndex < tweetIds.length ? tweetIds[currentIndex] : null;\nconst hasMore = currentIndex < tweetIds.length;\n\nreturn {\n\tcurrentTweetId,\n\thasMore,\n\tcurrentIndex,\n\ttotalTweets: tweetIds.length\n};"
      },
      "outputs": {
        "result": null,
        "error": ""
